    "client" : [
      {
        "adapter": "kotlin",
        "value": "generations.gg.generations.core.generationscore.fabric.client.GenerationsCoreClientFabric"
      }
    ],
    "fabric-datagen": ["generations.gg.generations.core.generationscore.fabric.datagen.DataGen"],
